<div class="table-responsive">
    <table class="table table-sm small">
        <thead class="thead-dark">
            <tr>
                <th colspan="3" scope="col">Structure</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td width="75%">Namespaces</td>
                <td width="15%" class="text-right">{{ project.data('phploc').namespaces }}</td>
                <td width="10%" class="text-right"></td>
            </tr>
            <tr>
                <td>Interfaces</td>
                <td class="text-right">{{ project.data('phploc').interfaces }}</td>
                <td class="text-right"></td>
            </tr>
            <tr>
                <td>Traits</td>
                <td class="text-right">{{ project.data('phploc').traits }}</td>
                <td class="text-right"></td>
            </tr>
