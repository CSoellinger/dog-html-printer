                <td>Classes</td>
                <td class="text-right">{{ project.data('phploc').classes }}</td>
                <td class="text-right"></td>
            </tr>
            <tr>
                <td>&nbsp;&nbsp;Abstract Classes</td>
                <td class="text-right">{{ project.data('phploc').abstractClasses }}</td>
                <td class="text-right">
                    {% if project.data('phploc').classes > 0 %}
                        ({{ (100 - (((project.data('phploc').classes - project.data('phploc').abstractClasses) / project.data('phploc').classes) * 100))|number_format(2, '.', '') }}%)
                    {% else %}
                        (0.00%)
                    {% endif %}
                </td>
            </tr>
            <tr>
                <td>&nbsp;&nbsp;Concrete Classes</td>
                <td class="text-right">{{ project.data('phploc').concreteClasses }}</td>
